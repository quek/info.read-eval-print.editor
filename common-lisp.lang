    <context id="clisp">
      <include>
        <context id="line-comment" style-ref="comment" end-at-line-end="true">
          <start>;</start>
          <include>
            <context ref="def:in-comment"/>
          </include>
        </context>

        <context id="block-comment" style-ref="comment" end-at-line-end="false">
          <start>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#\|</start>
          <end>\|#</end>
          <include>
            <context id="nested-block-comment" style-ref="comment" end-at-line-end="false">
              <start>#\|</start>
              <end>\|#</end>
              <include>
                <context ref="nested-block-comment"/>
              </include>
            </context>
          </include>
        </context>

        <context id="invalid" style-ref="invalid">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])(#[&lt;) \t]|#$)</match>
        </context>

        <context id="string" style-ref="string" end-at-line-end="false">
          <start>"</start>
          <end>"</end>
          <include>
            <context id="escaped" style-ref="string-esc" end-at-line-end="true">
              <start>\\</start>
              <end>.</end>
            </context>
          </include>
        </context>

        <context id="std-char" style-ref="std-char">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#\\(.(?![\w\d*+_/&amp;&lt;>=\.-])|$)</match>
        </context>

        <context id="spec-char" style-ref="std-char">
          <start>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#\\</start>
          <end>(?![\w\d*+_/&amp;&lt;>=\.-])</end>
          <include>
            <context id="spec-char-name" style-ref="spec-char">
              <match>[\w\d*+_/&amp;&lt;>=-]{2,}</match>
            </context>
          </include>
        </context>

        <context id="float" style-ref="float">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])(([+-]?\d*\.\d+([dDeEfFlLsS][+-]?\d+)?)|([+-]?\d+(\.\d*)?[dDeEfFlLsS][+-]?\d+))(?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="bitvec" style-ref="bitvec">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#\d*\*[01]+(?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="rat-decimal" style-ref="rat-decimal">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])[+-]?\d+(/\d+)?(?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="rat-binary" style-ref="rat-binary">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#[bB][+-]?[01]+(/[01]+)?(?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="rat-octal" style-ref="rat-octal">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#[oO][+-]?[0-7]+(/[0-7]+)?(?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="rat-hex" style-ref="rat-hex">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#[xX][+-]?[0-9a-fA-F]+(/[0-9a-fA-F]+)?(?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="rat-basen" style-ref="rat-basen">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#[rR][+-]?[0-9a-zA-Z]+(/[0-9a-zA-Z]+)?(?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="symbol" style-ref="symbol">
          <start>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#:</start>
          <end>(?![\w\d*+_/&amp;&lt;>=\.-])</end>
          <include>
            <context id="symbol-singleesc" end-at-line-end="true">
              <start>\\</start>
              <end>.</end>
            </context>
            <context id="symbol-multipleesc" end-at-line-end="false">
              <start>\|</start>
              <end>\|</end>
              <include>
                <context ref="symbol-singleesc"/>
              </include>
            </context>
          </include>
        </context>

        <context id="keyword" style-ref="keyword">
          <start>(?&lt;![\w\d*+_/&amp;&lt;>=\.-:]):</start>
          <end>(?![\w\d*+_/&amp;&lt;>=\.-])</end>
          <include>
            <context id="keyword-singleesc" end-at-line-end="true">
              <start>\\</start>
              <end>.</end>
            </context>
            <context id="keyword-multipleesc" end-at-line-end="false">
              <start>\|</start>
              <end>\|</end>
              <include>
                <context ref="keyword-singleesc"/>
              </include>
            </context>
          </include>
        </context>

        <context id="sharpeqnot" style-ref="sharpeqnot">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#\d+[=#](?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="featexpr" style-ref="featexpr">
          <match>(?&lt;![\w\d*+_/&amp;&lt;>=\.-])#[+-][\w\d*+_/&amp;&lt;>=-]*(?![\w\d*+_/&amp;&lt;>=\.-])</match>
        </context>

        <context id="singleesc" style-ref="escapeseq" end-at-line-end="true">
          <start>\\</start>
          <end>.</end>
        </context>

        <context id="multipleesc" style-ref="escapeseq" end-at-line-end="false">
          <start>\|</start>
          <end>\|</end>
          <include>
            <context ref="escaped"/>
          </include>
        </context>

